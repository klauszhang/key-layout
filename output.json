        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "right_control"
              ]
            },
            "key_code": "w"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "up_arrow"
            }
          ]
        }
      ]
    },
    {
